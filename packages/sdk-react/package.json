{
  "name": "@openweb3-apps/sdk-react",
  "version": "2.0.9",
  "description": "React bindings for Mini Apps client SDK. Contains hooks, components and other useful tools which allow usage of React along with Mini Apps client SDK.",
  "author": "Vladislav Kibenko <wolfram.deus@gmail.com>",
  "homepage": "https://github.com/openweb3-io/miniapps#readme",
  "repository": {
    "type": "git",
    "url": "git@github.com:openweb3-io/miniapps.git",
    "directory": "packages/sdk-react"
  },
  "bugs": {
    "url": "https://github.com/openweb3-io/miniapps/issues"
  },
  "keywords": [
    "openweb3-mini-apps",
    "typescript",
    "sdk",
    "react"
  ],
  "license": "MIT",
  "type": "module",
  "sideEffects": false,
  "files": [
    "dist",
    "src",
    "tsconfig.json"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/dts/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/dts/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.cjs"
    }
  },
  "scripts": {
    "typecheck": "tsc --noEmit",
    "build": "vite build"
  },
  "dependencies": {
    "@openweb3-apps/sdk": "workspace:^"
  },
  "peerDependencies": {
    "@types/react": "^17.0.0 || ^18.0.0",
    "react": "^17.0.0 || ^18.0.0"
  },
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    }
  },
  "devDependencies": {
    "tsconfig": "workspace:*",
    "@types/react": "^18.0.0",
    "react": "^18.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
