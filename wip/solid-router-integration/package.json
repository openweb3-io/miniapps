{
  "name": "@openweb3-apps/solid-router-integration",
  "version": "1.0.1",
  "description": "@solidjs/router integration for navigation.",
  "author": "Dev <dev@openweb3.io>",
  "homepage": "https://github.com/openweb3-io/miniapps#readme",
  "repository": {
    "type": "git",
    "url": "git@github.com:openweb3-io/miniapps.git",
    "directory": "packages/solid-router-integration"
  },
  "bugs": {
    "url": "https://github.com/openweb3-io/miniapps/issues"
  },
  "keywords": [
    "openweb3-mini-apps",
    "typescript",
    "navigation",
    "solidjs"
  ],
  "license": "MIT",
  "type": "module",
  "sideEffects": false,
  "files": [
    "dist",
    "src",
    "tsconfig.json"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/dts/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/dts/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.cjs"
    }
  },
  "scripts": {
    "lint": "cd ../.. && eslint packages/solid-router-integration/src --ignore-pattern **/*.test.ts",
    "lint:fix": "pnpm run lint --fix",
    "typecheck": "tsc --noEmit",
    "build": "vite build"
  },
  "peerDependencies": {
    "@solidjs/router": "^0.14.3",
    "@openweb3-apps/navigation": "workspace:^",
    "solid-js": "^1.8.16"
  },
  "devDependencies": {
    "tsconfig": "workspace:*"
  },
  "publishConfig": {
    "access": "public"
  }
}
